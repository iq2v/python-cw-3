favorite_animals = {"dog" , 'cat' , "monkey" , "rabbit"}
print("favourite_animals")
print(favourite_animals{1})
favorite_animals.remove("monkey")
favorite_animals.remove("bat")
for animal in favorite_animals : 
    print(f"i love {animal}")
    numbers = {5,4,3,2,1} 
    sum = 0 
    for num in numbers : 
        sum += num 
        print(sum)

